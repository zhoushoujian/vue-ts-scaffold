{
  "name": "vue2-ts-scaffold",
  "private": true,
  "version": "1.0.0",
  "description": "Vue 2 + typescript 脚手架, 默认使用webpack，可选用vite开发",
  "scripts": {
    "lint": "eslint -c .eslintrc.js \"./src/**/*.{js,ts,vue}\" --fix",
    "typeCheck": "tsc --noEmit",
    "codeCheck": "npm run lint && npm run typeCheck",
    "dev": "cross-env NODE_ENV=development webpack serve --config ./webpack/dev.ts",
    "start": "npm run dev",
    "analyzer": "webpack --config webpack/analyzer.ts",
    "build:prod": "webpack --config webpack/prod.ts",
    "build": "npm run build:prod",
    "vite": "vite",
    "vite:build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 vite build",
    "vite:build:analyzer": "cross-env NODE_OPTIONS=--max_old_space_size=4096 type=analyzer vite build",
    "prepare": "husky install"
  },
  "homepage": "https://github.com/zhoushoujian/vue-ts-scaffold/tree/release/vue2",
  "dependencies": {
    "axios": "^1.6.2",
    "core-js": "^3.34.0",
    "custom-event-polyfill": "^1.0.7",
    "element-ui": "^2.15.14",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "qiankun": "^2.10.16",
    "vue": "^2.7.14",
    "vue-router": "^3.5.4",
    "whatwg-fetch": "^3.6.20"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.22.15",
    "@babel/runtime": "^7.21.5",
    "@babel/runtime-corejs3": "^7.12.5",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@szhou/eslint-config": "^1.4.3",
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.6",
    "@types/circular-dependency-plugin": "^5.0.5",
    "@types/node": "16.11.7",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vue/cli-plugin-babel": "^4.5.19",
    "babel-loader": "^9.1.3",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^4.3.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.4.5",
    "path-browserify": "^1.0.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-visualizer": "^5.11.0",
    "sass": "^1.57.1",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.3",
    "terser": "5.4.0",
    "thread-loader": "^4.0.2",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vue-eslint-parser": "9.3.2",
    "vue-hot-reload-api": "^2.2.0",
    "vue-loader": "^15.10.0",
    "webpack": "^5.64.1",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.5.0",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": [
    "> 1%",
    "last 6 versions",
    "ie 11"
  ]
}