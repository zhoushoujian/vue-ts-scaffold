{
  "name": "vue3-ts-scaffold",
  "private": true,
  "version": "1.0.0",
  "description": "Vue 3 + typescript 脚手架, 默认使用webpack，可选用vite开发",
  "scripts": {
    "lint": "eslint -c .eslintrc.js \"./src/**/*.{js,ts,vue}\" --fix",
    "typeCheck": "vue-tsc --noEmit",
    "codeCheck": "concurrently \"npm run lint\" \"npm run typeCheck\"",
    "versionCheck": "npx specified-package-version-check",
    "dev": "cross-env NODE_ENV=development webpack serve --config ./webpack/dev.ts",
    "start": "concurrently \"npm run dev\" \"npm run versionCheck\"",
    "analyzer": "webpack --config webpack/analyzer.ts",
    "build:prod": "webpack --config webpack/prod.ts",
    "build": "npm run build:prod",
    "postbuild": "script-tools manifest",
    "vite": "concurrently \"vite\" \"npm run versionCheck\"",
    "vite:build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 vite build",
    "vite:build:analyzer": "cross-env NODE_OPTIONS=--max_old_space_size=4096 type=analyzer vite build",
    "prepare": "husky install"
  },
  "homepage": "https://github.com/zhoushoujian/vue-ts-scaffold/tree/release/vue3",
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@szhou/components": "^1.5.9",
    "@szhou/script-tools": "^1.3.8",
    "axios": "^1.6.2",
    "element-plus": "2.6.0",
    "jsencrypt": "^3.3.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.22.15",
    "@babel/runtime": "^7.21.5",
    "@babel/runtime-corejs3": "^7.12.5",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@szhou/eslint-config": "^1.4.9",
    "@szhou/specified-package-version-check": "^3.4.10",
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.6",
    "@types/circular-dependency-plugin": "^5.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.11.9",
    "@types/numeral": "^2.0.5",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/compiler-sfc": "^3.3.11",
    "async-validator": "^4.2.5",
    "babel-loader": "^9.1.3",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.34.0",
    "cross-env": "^7.0.3",
    "css-loader": "^4.3.0",
    "fork-ts-checker-webpack-plugin": "^6.5.2",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.4.5",
